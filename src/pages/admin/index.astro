---
import CMS from 'decap-cms-app'; // eslint-disable-line @typescript-eslint/no-unused-vars
import setupCMS from './cms'; // eslint-disable-line @typescript-eslint/no-unused-vars
---
<!doctype html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="robots" content="noindex" />
    <title>Fog City Jazz CMS</title>
    <link href="/admin/config.yml" type="text/yaml" rel="cms-config-url" />
    <link rel="icon" href="/favicon.svg" />
  </head>
  <body>
    <script>
      /* global CMS, setupCMS */
      setupCMS(CMS);
      CMS.init();
    </script>
  </body>
</html>
